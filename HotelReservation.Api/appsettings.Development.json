{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information"
      }
    },
    "WriteTo": [
      { "Name": "Console" }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ]
  },
  "Jwt": {
    "Secret": "super-duper-secret-value-that-should-be-in-user-secrets",
    "Issuer": "bmez",
    "Audience": "developers",
    "ExpirationInMinutes": 5
  },
  "SmtpConfiguration": {
    "Host": "localhost",
    "Port": "25",
    "User": "",
    "Key": "",
    "Sender": "hotelreservation@mail.com"
  }
}
